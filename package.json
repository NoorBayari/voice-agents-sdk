{
  "name": "@hamsa-ai/voice-agents-sdk",
  "version": "0.4.0-beta.7",
  "description": "Hamsa AI - Voice Agents JavaScript SDK",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "types": "types/main.d.ts",
  "exports": {
    ".": {
      "types": "./types/main.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "browser": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist",
    "types",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist types",
    "build:types": "tsc -p tsconfig.declarations.json",
    "build:rollup": "rollup -c",
    "build": "npm run clean && npm run build:types && npm run build:rollup",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "npm run build"
  },
  "author": "Hamsa AI Inc.",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@biomejs/biome": "^2.2.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^30.0.0",
    "babel-jest": "^29.7.0",
    "buffer": "^6.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lefthook": "^1.12.3",
    "lint-staged": "^16.1.5",
    "rimraf": "^6.0.1",
    "rollup": "^4.48.1",
    "ts-jest": "^29.4.1",
    "typescript": "^5.5.4",
    "ultracite": "^5.2.3"
  },
  "dependencies": {
    "events": "^3.3.0",
    "livekit-client": "^2.15.4",
    "jwt-decode": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hamsa-ai/voice-agents-sdk.git"
  },
  "keywords": [
    "arab",
    "voice",
    "agent",
    "web",
    "nodejs",
    "sdk",
    "hamsa",
    "ai"
  ],
  "bugs": {
    "url": "https://github.com/hamsa-ai/voice-agents-sdk/issues"
  },
  "homepage": "https://github.com/hamsa-ai/voice-agents-sdk#readme",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "npx ultracite format"
    ]
  }
}
